<core:FragmentDefinition 
	xmlns="sap.m" 
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
	xmlns:semantic="sap.m.semantic">
	
	<Dialog >
	
		<content>
			<f:SimpleForm id="SimpleFormChange300"
			editable="true"
			layout="ResponsiveGridLayout"
			title="Add Consultant Details"
			labelSpanXL="3"
			labelSpanL="3"
			labelSpanM="3"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="4"
			emptySpanL="4"
			emptySpanM="4"
			emptySpanS="0"
			columnsXL="1"
			columnsL="1"
			columnsM="1"
			singleContainerFullSize="false" >
			<f:content>
				<Label text="Name" />
				<Input id="c_Name" />
				<Label text="Surname" />
				<Input id="c_Surname" />
				<Label text="Email" />
				<Input id="c_email" />
				<Label text="Phone Number" />
				<Input id="c_Cell" />
						
			</f:content>
		</f:SimpleForm>
		</content>
		<beginButton>
			<Button text="Submit" press="onSubmitConsultant" />
		</beginButton>
		<endButton>
			<Button text="Close" press="onClose" />
		</endButton>
	</Dialog>
	
</core:FragmentDefinition>